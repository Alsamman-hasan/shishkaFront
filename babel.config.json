{
  "presets": [
    ["@babel/preset-env", {"targets": {"node": "current", "esmodules": true}}],
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
   "plugins": [
      [
        "babel-plugin-import",
        {
          "libraryName": "@mui/material",
          "libraryDirectory": "",
          "camel2DashComponentName": false
        },
        "core"
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "@mui/icons-material",
          "libraryDirectory": "",
          "camel2DashComponentName": false
        },
        "icons"
      ]
   ]
}